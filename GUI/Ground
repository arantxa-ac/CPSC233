import java.awt.Rectangle;
import javafx.scene.image.Image;
import javafx.scene.layout.Pane;
import javafx.util.Duration;

public class Ground extends Sprite{

	private final Image IMAGE = new Image("ground.png");
    private final int COUNT = 6;
    private final int COLUMNS = 6;
    private final int OFFSET_X = 0;
    private final int OFFSET_Y = 0;
    private final int WIDTH = 200;
    private final int HEIGHT = 14;
    private final int MILLISDURATION = 2000;
    
	public Ground(Pane layer, Image image, int x, int y) {
		super(layer, image, x, y);
		animate(IMAGE, Duration.millis(MILLISDURATION), COUNT, COLUMNS, OFFSET_X, OFFSET_Y, WIDTH, HEIGHT);
	}
	
